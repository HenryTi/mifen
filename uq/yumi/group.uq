ENUM EnumGroupType (normal=0, black=-1, all=1);

ID Group (
	id,
	key no,
	name char(50),
	type Enum EnumGroupType DEFAULT 0,
);

IX UserGroup (
	ix user,
	id Group,
);

IX UserAllStock (
	ix user,
	id Stock,
);

IX UserBlockStock (
	ix user,
	id Stock,
);

IX GroupStock (
	ix,				-- group
	id,			-- Stock
	order SMALLINT,
);
